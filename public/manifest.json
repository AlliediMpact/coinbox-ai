{
  "name": "CoinBox AI - P2P Financial Platform",
  "short_name": "CoinBox AI",
  "description": "Secure peer-to-peer financial transactions with coin-based marketplace for investments and loans",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#193281",
  "background_color": "#ffffff",
  "categories": ["finance", "business", "productivity"],
  "lang": "en",
  "dir": "ltr",
  "icons": [
    {
      "src": "/assets/coinbox-ai.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/assets/coinbox-ai.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/assets/coinbox-ai.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "/assets/screenshot-mobile.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "CoinBox AI Dashboard on Mobile"
    },
    {
      "src": "/assets/screenshot-desktop.png",
      "sizes": "1920x1080",
      "type": "image/png",
      "form_factor": "wide",
      "label": "CoinBox AI Dashboard on Desktop"
    }
  ],
  "features": [
    "cross-origin-isolated",
    "web-share",
    "web-share-target"
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "Access your trading dashboard",
      "url": "/dashboard",
      "icons": [{ "src": "/assets/coinbox-ai.png", "sizes": "96x96" }]
    },
    {
      "name": "Trade",
      "short_name": "Trade",
      "description": "Start trading coins",
      "url": "/dashboard/trading",
      "icons": [{ "src": "/assets/coinbox-ai.png", "sizes": "96x96" }]
    },
    {
      "name": "Wallet",
      "short_name": "Wallet",
      "description": "View your wallet",
      "url": "/dashboard/wallet",
      "icons": [{ "src": "/assets/coinbox-ai.png", "sizes": "96x96" }]
    },
    {
      "name": "Referrals",
      "short_name": "Referrals",
      "description": "Track your referrals",
      "url": "/dashboard/referral",
      "icons": [{ "src": "/assets/coinbox-ai.png", "sizes": "96x96" }]
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.coinboxai.app",
      "id": "com.coinboxai.app"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/coinbox-ai/id123456789",
      "id": "123456789"
    }
  ],
  "prefer_related_applications": false,
  "share_target": {
    "action": "/dashboard/share",
    "method": "POST",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+coinbox",
      "url": "/dashboard?action=%s"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred"
}
